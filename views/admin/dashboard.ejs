<%- include ('layout/navbar.ejs') %>
      <main class="relative h-full max-h-screen transition-all duration-200 ease-in-out xl:ml-68 rounded-xl">
            <!-- Navbar -->
            <%- include ('layout/header.ejs') %>
                  <!-- end Navbar -->
                  <!-- cards -->
                  <div class="w-full px-6 py-6 mx-auto">
                        <!-- row 1 -->
                        <div class="flex flex-wrap -mx-3">
                              <!-- card1 -->
                              <div class="w-full max-w-full px-3 mb-6 sm:w-1/2 sm:flex-none xl:mb-0 xl:w-1/4">
                                    <div
                                          class="relative flex flex-col min-w-0 break-words bg-white shadow-xl dark:bg-slate-850 dark:shadow-dark-xl rounded-2xl bg-clip-border">
                                          <div class="flex-auto p-4">
                                                <div class="flex flex-row -mx-3">
                                                      <div class="flex-none w-2/3 max-w-full px-3">
                                                            <div>
                                                                  <p
                                                                        class="mb-0 font-sans text-sm font-semibold leading-normal uppercase dark:text-white dark:opacity-60">
                                                                        Total's Sales</p>
                                                                  <% if (totalSalesAmount>0) { %>
                                                                        <h5 class="mb-2 font-bold dark:text-white">
                                                                              â‚¹ <%= totalSalesAmount %>
                                                                        </h5>
                                                                        <% } else { %>
                                                                              <h5
                                                                                    class="mb-2 font-bold dark:text-white">
                                                                                    0
                                                                              </h5>
                                                                              <% } %>
                                                                                    <p
                                                                                          class="mb-0 dark:text-white dark:opacity-60">
                                                                                          <span
                                                                                                class="text-sm font-bold leading-normal text-emerald-500">+55%</span>
                                                                                          since today
                                                                                    </p>
                                                            </div>
                                                      </div>
                                                      <div class="px-3 text-right basis-1/3">
                                                            <div
                                                                  class="inline-block w-12 h-12 text-center rounded-circle bg-gradient-to-tl from-blue-500 to-violet-500">
                                                                  <a href="/admin/salesreport">
                                                                        <i
                                                                              class="ni leading-none ni-money-coins text-lg relative top-3.5 text-white"></i>
                                                                  </a>
                                                            </div>
                                                      </div>
                                                </div>
                                          </div>
                                    </div>
                              </div>

                              <!-- card2 -->
                              <div class="w-full max-w-full px-3 mb-6 sm:w-1/2 sm:flex-none xl:mb-0 xl:w-1/4">
                                    <div
                                          class="relative flex flex-col min-w-0 break-words bg-white shadow-xl dark:bg-slate-850 dark:shadow-dark-xl rounded-2xl bg-clip-border">
                                          <div class="flex-auto p-4">
                                                <div class="flex flex-row -mx-3">
                                                      <div class="flex-none w-2/3 max-w-full px-3">
                                                            <div>
                                                                  <p
                                                                        class="mb-0 font-sans text-sm font-semibold leading-normal uppercase dark:text-white dark:opacity-60">
                                                                        Total Users</p>
                                                                  <% if (user>0) { %>
                                                                        <h5 class="mb-2 font-bold dark:text-white">
                                                                              <%= user %>
                                                                        </h5>
                                                                        <% } else { %>
                                                                              <h5
                                                                                    class="mb-2 font-bold dark:text-white">
                                                                                    0</h5>
                                                                              <% } %>

                                                                                    <p
                                                                                          class="mb-0 dark:text-white dark:opacity-60">
                                                                                          <span
                                                                                                class="text-sm font-bold leading-normal text-emerald-500">+3%</span>
                                                                                          since today
                                                                                    </p>
                                                            </div>
                                                      </div>
                                                      <div class="px-3 text-right basis-1/3">
                                                            <div
                                                                  class="inline-block w-12 h-12 text-center rounded-circle bg-gradient-to-tl from-red-600 to-orange-600">
                                                                  <a href="/admin/user">
                                                                        <i
                                                                              class="ni leading-none ni-world text-lg relative top-3.5 text-white"></i>
                                                                  </a>
                                                            </div>
                                                      </div>
                                                </div>
                                          </div>
                                    </div>
                              </div>

                              <!-- card3 -->
                              <div class="w-full max-w-full px-3 mb-6 sm:w-1/2 sm:flex-none xl:mb-0 xl:w-1/4">
                                    <div
                                          class="relative flex flex-col min-w-0 break-words bg-white shadow-xl dark:bg-slate-850 dark:shadow-dark-xl rounded-2xl bg-clip-border">
                                          <div class="flex-auto p-4">
                                                <div class="flex flex-row -mx-3">
                                                      <div class="flex-none w-2/3 max-w-full px-3">
                                                            <div>
                                                                  <p
                                                                        class="mb-0 font-sans text-sm font-semibold leading-normal uppercase dark:text-white dark:opacity-60">
                                                                        New Orders</p>
                                                                  <% if (order>0) { %>
                                                                        <h5 class="mb-2 font-bold dark:text-white">
                                                                              +<%= order %>
                                                                        </h5>
                                                                        <% } else { %>
                                                                              <h5
                                                                                    class="mb-2 font-bold dark:text-white">
                                                                                    0</h5>
                                                                              <% } %>
                                                                                    <p
                                                                                          class="mb-0 dark:text-white dark:opacity-60">
                                                                                          <span
                                                                                                class="text-sm font-bold leading-normal text-red-600">-2%</span>
                                                                                          since last quarter
                                                                                    </p>
                                                            </div>
                                                      </div>
                                                      <div class="px-3 text-right basis-1/3">
                                                            <div
                                                                  class="inline-block w-12 h-12 text-center rounded-circle bg-gradient-to-tl from-emerald-500 to-teal-400">
                                                                  <a href="/admin/order">
                                                                        <i
                                                                              class="ni leading-none ni-paper-diploma text-lg relative top-3.5 text-white"></i>
                                                                  </a>
                                                            </div>
                                                      </div>
                                                </div>
                                          </div>
                                    </div>
                              </div>

                              <!-- card4 -->
                              <div class="w-full max-w-full px-3 sm:w-1/2 sm:flex-none xl:w-1/4">
                                    <div
                                          class="relative flex flex-col min-w-0 break-words bg-white shadow-xl dark:bg-slate-850 dark:shadow-dark-xl rounded-2xl bg-clip-border">
                                          <div class="flex-auto p-4">
                                                <div class="flex flex-row -mx-3">
                                                      <div class="flex-none w-2/3 max-w-full px-3">
                                                            <div>
                                                                  <p
                                                                        class="mb-0 font-sans text-sm font-semibold leading-normal uppercase dark:text-white dark:opacity-60">
                                                                        TOTAL PRODUCT
                                                                        <% if (product) { %>
                                                                              <h5
                                                                                    class="mb-2 font-bold dark:text-white">
                                                                                    <%= product %>
                                                                              </h5>
                                                                              <% } else { %>
                                                                                    <h5
                                                                                          class="mb-2 font-bold dark:text-white">
                                                                                          0
                                                                                    </h5>
                                                                                    <% } %>
                                                                                          <p
                                                                                                class="mb-0 dark:text-white dark:opacity-60">
                                                                                                <span
                                                                                                      class="text-sm font-bold leading-normal text-emerald-500">+5%</span>
                                                                                                than higher month
                                                                                          </p>
                                                            </div>
                                                      </div>
                                                      <div class="px-3 text-right basis-1/3">
                                                            <div
                                                                  class="inline-block w-12 h-12 text-center rounded-circle bg-gradient-to-tl from-orange-500 to-yellow-500">
                                                                  <a href="/admin/Product">
                                                                        <i
                                                                              class="ni leading-none ni-cart text-lg relative top-3.5 text-white"></i>
                                                                  </a>

                                                            </div>
                                                      </div>
                                                </div>
                                          </div>
                                    </div>
                              </div>
                        </div>

                        <!-- cards row 2 -->
                        <div class="flex flex-wrap mt-6 -mx-3">
                              <div class="w-full max-w-full px-3 mt-0 lg:w-7/12 lg:flex-none">
                                    <div
                                          class="border-black/12.5 dark:bg-slate-850 dark:shadow-dark-xl shadow-xl relative z-20 flex min-w-0 flex-col break-words rounded-2xl border-0 border-solid bg-white bg-clip-border">
                                          <div
                                                class="border-black/12.5 mb-0 rounded-t-2xl border-b-0 border-solid p-6 pt-4 pb-0">
                                                <h6 class="capitalize dark:text-white">Sales overview</h6>
                                                <p class="mb-0 text-sm leading-normal dark:text-white dark:opacity-60">
                                                      <i class="fa fa-arrow-up text-emerald-500"></i>
                                                      <span class="font-semibold">4% more</span> in 2021
                                                </p>
                                          </div>
                                          <div class="flex-auto p-4">
                                                <div>
                                                      <canvas id="chart-line" height="300"></canvas>
                                                </div>
                                          </div>
                                    </div>
                              </div>

                              <div class="w-full max-w-full px-3 lg:w-5/12 lg:flex-none">
                                    <div
                                          class="border-black/12.5 shadow-xl dark:bg-slate-850 dark:shadow-dark-xl relative flex min-w-0 flex-col break-words rounded-2xl border-0 border-solid bg-white bg-clip-border">
                                          <div class="p-4 pb-0 rounded-t-4">
                                                <h6 class="mb-0 dark:text-white">Categories</h6>
                                          </div>
                                          <div class="flex-auto p-4">
                                                <ul class="flex flex-col pl-0 mb-0 rounded-lg">
                                                      <%category.forEach(category=> { %>
                                                            <li
                                                                  class="relative flex justify-between py-2 pr-4 mb-2 border-0 rounded-t-lg rounded-xl text-inherit">
                                                                  <div class="flex items-center">
                                                                        <div
                                                                              class="inline-block w-8 h-8 mr-4 text-center text-black bg-center shadow-sm fill-current stroke-none bg-gradient-to-tl from-zinc-800 to-zinc-700 dark:bg-gradient-to-tl dark:from-slate-750 dark:to-gray-850 rounded-xl">

                                                                              <img src="<%= category.categoryImage %> "
                                                                                    class="rounded-xl" />
                                                                        </div>
                                                                        <div class="flex flex-col">
                                                                              <h6
                                                                                    class="mb-1 text-sm leading-normal text-slate-700 dark:text-white">
                                                                                    <%= category.categoryName %>
                                                                              </h6>
                                                                              <span
                                                                                    class="text-xs leading-tight dark:text-white/80">250
                                                                                    in stock, <span
                                                                                          class="font-semibold">346+
                                                                                          sold</span></span>
                                                                        </div>
                                                                  </div>

                                                            </li>
                                                            <% }) %>
                                                </ul>
                                          </div>

                                    </div>
                              </div>
                        </div>

                        <!-- cards row 3 -->

                        <div class="flex flex-wrap mt-6 -mx-3">
                              <div class="w-full max-w-full px-3 mt-0 mb-6 lg:mb-0 lg:w-7/12 lg:flex-none">
                                    <div
                                          class="relative flex flex-col min-w-0 break-words bg-white border-0 border-solid shadow-xl dark:bg-slate-850 dark:shadow-dark-xl dark:bg-gray-950 border-black-125 rounded-2xl bg-clip-border">
                                          <div class="p-4 pb-0 mb-0 rounded-t-4">
                                                <div class="flex justify-between">
                                                      <h6 class="mb-2 dark:text-white"> Last Product</h6>
                                                </div>
                                          </div>
                                          <div class="overflow-x-auto">
                                                <table
                                                      class="items-center w-full mb-4 align-top border-collapse border-gray-200 dark:border-white/40">
                                                      <tbody>
                                                            <% productList.forEach(product=> { %>
                                                                  <tr>
                                                                        <td
                                                                              class="p-2 align-middle bg-transparent border-b w-3/10 whitespace-nowrap dark:border-white/40">
                                                                              <div class="flex items-center px-2 py-1">
                                                                                    <div>
                                                                                          <img src="<%= product.images[0].Location %> "
                                                                                                alt="Product Image"
                                                                                                style="width: 30px;" />
                                                                                    </div>
                                                                                    <div class="ml-6">
                                                                                          <p
                                                                                                class="mb-0 text-xs font-semibold leading-tight dark:text-white dark:opacity-60">
                                                                                                Product Name:</p>
                                                                                          <h6
                                                                                                class="mb-0 text-sm leading-normal dark:text-white">
                                                                                                <%= product.Product_title
                                                                                                      %>
                                                                                          </h6>
                                                                                    </div>
                                                                              </div>
                                                                        </td>
                                                                        <td
                                                                              class="p-2 align-middle bg-transparent border-b whitespace-nowrap dark:border-white/40">
                                                                              <div class="text-center">
                                                                                    <p
                                                                                          class="mb-0 text-xs font-semibold leading-tight dark:text-white dark:opacity-60">
                                                                                          Price:</p>
                                                                                    <h6
                                                                                          class="mb-0 text-sm leading-normal dark:text-white">
                                                                                          â‚¹ <%= product.Price.Offer_price
                                                                                                %>
                                                                                    </h6>
                                                                              </div>
                                                                        </td>
                                                                        <td
                                                                              class="p-2 align-middle bg-transparent border-b whitespace-nowrap dark:border-white/40">
                                                                              <div class="text-center">
                                                                                    <p
                                                                                          class="mb-0 text-xs font-semibold leading-tight dark:text-white dark:opacity-60">
                                                                                          Quantity:</p>
                                                                                    <h6
                                                                                          class="mb-0 text-sm leading-normal dark:text-white">
                                                                                          <%= product.Product_quantity
                                                                                                %>
                                                                                    </h6>
                                                                              </div>
                                                                        </td>
                                                                        <td
                                                                              class="p-2 text-sm leading-normal align-middle bg-transparent border-b whitespace-nowrap dark:border-white/40">
                                                                              <div class="flex-1 text-center">
                                                                                    <p
                                                                                          class="mb-0 text-xs font-semibold leading-tight dark:text-white dark:opacity-60">
                                                                                          Color:</p>
                                                                                    <h6
                                                                                          class="mb-0 text-sm leading-normal dark:text-white">
                                                                                          <%= product.color %>
                                                                                    </h6>
                                                                              </div>
                                                                        </td>
                                                                  </tr>
                                                                  <% }) %>
                                                      </tbody>
                                                </table>
                                          </div>
                                    </div>
                              </div>
                              <div class="w-full max-w-full px-3 mt-0 lg:w-5/12 lg:flex-none">
                                    <div
                                          class="border-black/12.5 shadow-xl dark:bg-slate-850 dark:shadow-dark-xl relative flex min-w-0 flex-col break-words rounded-2xl border-0 border-solid bg-white bg-clip-border">
                                          <div class="p-4 pb-0 rounded-t-4">
                                                <h6 class="mb-0 dark:text-white">Orders</h6>
                                          </div>
                                          <div class="flex-auto p-4">
                                                <ul class="flex flex-col pl-0 mb-0 rounded-lg">
                                                      <% orderList.forEach(order=> { %>
                                                            <li
                                                                  class="relative flex justify-between py-2 pr-4 mb-2 border-0 rounded-t-lg rounded-xl text-inherit">
                                                                  <div class="flex items-center">
                                                                        <div
                                                                              class="inline-block w-8 h-8 mr-4 text-center text-black bg-center shadow-sm fill-current stroke-none bg-gradient-to-tl from-zinc-800 to-zinc-700 dark:bg-gradient-to-tl dark:from-slate-750 dark:to-gray-850 rounded-xl">
                                                                              <i
                                                                                    class="text-white ni ni-mobile-button relative top-0.75 text-xxs"></i>
                                                                        </div>
                                                                        <div class="flex flex-col">
                                                                              <h6
                                                                                    class="mb-1 text-sm leading-normal text-slate-700 dark:text-white">
                                                                                    <%= order._id %>
                                                                              </h6>
                                                                              <span
                                                                                    class="text-xs leading-tight dark:text-white/80">
                                                                                    <%= order.orderItems.length %>
                                                                                          products, <span
                                                                                                class="font-semibold">Status:
                                                                                                <%= order.deliveryStatus
                                                                                                      %>
                                                                                          </span>
                                                                              </span>
                                                                        </div>
                                                                  </div>
                                                                  <div class="flex">
                                                                        <a href="/admin/order"><button
                                                                                    class="group ease-in leading-pro text-xs rounded-3.5xl p-1.2 h-6.5 w-6.5 mx-0 my-auto inline-block cursor-pointer border-0 bg-transparent text-center align-middle font-bold text-slate-700 shadow-none transition-all dark:text-white"><i
                                                                                          class="ni ease-bounce text-2xs group-hover:translate-x-1.25 ni-bold-right transition-all duration-200"
                                                                                          aria-hidden="true"></i></button></a>
                                                                  </div>
                                                            </li>
                                                            <% }) %>
                                                </ul>
                                          </div>
                                    </div>
                              </div>
                        </div>


                  </div>
                  <!-- end cards -->
      </main>
      </div>
      <%- include ('layout/footer.ejs') %>